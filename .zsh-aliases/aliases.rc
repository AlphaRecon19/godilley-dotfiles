alias clip='xclip -selection clipboard'
alias con="bin/console"
alias curldump='curl -L -s -v -o/dev/null '
alias ssh_to_clipboard="cat ~/.ssh/id_rsa.pub| xclip -selection clipboard"
alias myssh="cat ~/.ssh/id_rsa.pub"
